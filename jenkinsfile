pipeline{
      agent any 
        stages{
           stage('install depencies'){
              steps{
                sh 'npm install '
                   }
                  }
            stage('test') {
                  steps{
                   sh 'echo "testing application..." '
                     }
                   }
             stage('deploy application'){
                   steps{
                       sh 'echo "deploying application...." '
          
                        }
                     }
   }
}
